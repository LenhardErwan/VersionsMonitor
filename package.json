{
  "name": "versions-monitor",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=prod HTTPS=false PORT=8012 node -r esm server/src/index.js --exec babel-node",
    "start-https": "cross-env NODE_ENV=prod HTTPS=true PORT=8012 node -r esm server/src/index.js --exec babel-node",
    "dev": "npm run dev-client & npm run dev-server",
    "build-dev": "webpack --mode development --config client/webpack.config.js --progress",
    "build-prod": "webpack --mode production --config client/webpack.config.js --progress",
    "dev-client": "webpack-dev-server -d --hot --config client/webpack.config.js --watch --env.ENDPOINT=http://localhost:8012",
    "dev-server": "cross-env NODE_ENV=debug PORT=8012 nodemon server/src/index.js --ignore 'server/ressources/*' --ignore 'client/*' --exec babel-node"
  },
  "keywords": [],
  "author": {
    "name": "Lenhard Erwan",
    "email": "contact@lenhard-erwan.fr",
    "url": "https://lenhard-erwan.fr"
  },
  "license": "GPL-3.0",
  "description": "Monitoring interface for the latest versions of any software or applications",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "toastr": "^2.1.4",
    "uikit": "^3.5.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsdom": "^16.3.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0"
  }
}
